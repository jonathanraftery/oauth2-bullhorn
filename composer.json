{
  "name": "jonathanraftery/oauth-bullhorn",
  "type": "library",
  "description": "Bullhorn OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
  "authors": [
    {
      "name": "Jonathan Raftery",
      "email": "jonathan@jonathan.raftery.com",
      "homepage": "https://github.com/jonathanraftery"
    }
  ],
  "keywords": ["bullhorn", "oauth", "oauth2", "client", "authorization"],
  "homepage": "https://github.com/jonathanraftery/oauth2-bullhorn",
  "license": "MIT",
  "require": {
    "league/oauth2-client": "^2.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "JonathanRaftery\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JonathanRaftery\\OAuth2\\Client\\Test\\": "tests/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
